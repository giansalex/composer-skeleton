{
    "name": "giansalex/composer-skeleton",
    "description": "Composer skeleton",
    "keywords": ["composer", "php"],
    "license": "MIT",
    "optimize-autoloader": true,
    "classmap-authoritative": true,
    "authors": [
        {
            "name": "Username",
            "email": "dev@domain.com",
	    "role" : "Developers"
        }
    ],
    "homepage": "https://github.com/giansalex/composer-skeleton",
    "type": "library",
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
	"autoload": {
        "psr-4": {
            "App\\": "src/App"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\App\\": "tests/App/"
        }
    }
}
